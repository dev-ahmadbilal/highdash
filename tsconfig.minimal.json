{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "removeComments": true,
    "declaration": false,
    "sourceMap": false,
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "noEmitOnError": true,
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
